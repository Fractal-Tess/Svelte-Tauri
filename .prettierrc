{
  "useTabs": false,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 80,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "semi": false,
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-toml"],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
